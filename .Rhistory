group_by(black_pop_vs_state) %>%
summarise(
sum_total_towns = n(),
sum_total_pop = sum(total_pop),
sum_total_players = sum(total_players)) %>%
mutate(pct_players = round((sum_total_players / sum(sum_total_players)) * 100, 1)) %>%
